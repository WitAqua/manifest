  <remote  name="gitlab"
           fetch="https://gitlab.com/" />

  <remote  name="witaqua"
           fetch="https://github.com/WitAqua"
           revision="15" />

  <remote  name="witaqua-devices"
           fetch="https://github.com/WitAqua-Devices"
           revision="15" />

  <!-- WitAqua -->
  <!-- Rootdir -->
